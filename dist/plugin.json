{
  "name": "vis-2-widgets-ecklicht",
  "version": "0.0.1",
  "type": "widget",
  "main": "dist/plugin.json",
  "title": "Ecklicht Control Plugin",
  "author": "mickyboys",
  "license": "MIT",
  "widgets": {
    "ecklicht": {
      "templates": [
        "ecklicht/ecklicht.html"
      ],
      "controls": {
        "ecklicht-control": {
          "type": "boolean",
          "name": "Ecklicht Control",
          "html": "ecklicht/ecklicht.html",
          "attrs": [
            {
              "name": "oid",
              "type": "id",
              "text": "State (Shelly Toggle)"
            },
            {
              "name": "oidBrightness",
              "type": "id",
              "text": "Brightness (0\u2013100)"
            },
            {
              "name": "oidTemp",
              "type": "id",
              "text": "Color Temp (0\u2013100)"
            },
            {
              "name": "customCss",
              "type": "textarea",
              "text": "Custom CSS"
            },
            {
              "name": "customJs",
              "type": "textarea",
              "text": "Custom JS"
            }
          ]
        }
      }
    }
  }
}